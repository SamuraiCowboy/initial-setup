[user]
    name = Venkata Rakesh Anne
    email = AVRC26@gmail.com

[color]
    ui = true
    diff = auto
    status = auto
    branch = auto

[color "diff"]
    meta = 108 dim  # cyan
    frag = 214 dim  # grey
    old = 208       # orange
    new = 142       # green

[color "status"]
    added = 142     # green
    changed = 208   # orange
    untracked = 167 # red
    branch = 108 bold blink # cyan

[color "branch"]
    current = 108 blink # cyan
    local = 172         # yellow
    remote = 175        # purple
[merge]
    tool = vimdiff
[diff]
	tool = vimdiff
[core]
    editor = vim
    excludesfile = ~/.gitignore_global
[difftool]
	prompt = false
[alias]
	d = difftool
