// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "ctrl+g ctrl+l",
        "command": "git.pull"
    },
    {
        "key": "ctrl+g ctrl+f",
        "command": "git.pullFrom"
    },
    {
        "key": "ctrl+g ctrl+h",
        "command": "git.push"
    },
    {
        "key": "ctrl+g ctrl+u",
        "command": "git.pushTo"
    },
    {
        "key": "ctrl+g ctrl+c",
        "command": "git.checkout"
    },
    {
        "key": "ctrl+g ctrl+t",
        "command": "git.fetch"
    },
    {
        "key": "ctrl+g ctrl+o",
        "command": "git.openChange"
    },
    {
        "key": "ctrl+g ctrl+y",
        "command": "git.sync"
    },
    {
        "key": "ctrl+g ctrl+g",
        "command": "git.commitStaged"
    },
    {
        "key": "ctrl+g ctrl+s",
        "command": "git.stage"
    },
    { 
        "key": "ctrl+shift+g",          
        "command": "workbench.view.scm" 
    },
    {
        "key": "ctrl+down",
        "command": "editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+down",
        "command": "-editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+up",
        "command": "editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+up",
        "command": "-editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },

]