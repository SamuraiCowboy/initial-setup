[user]
    name = Venkata Rakesh Anne
    email = AVRC26@gmail.com

[color]
    ui = true
    diff = auto
    status = auto
    branch = auto

[color "diff"]
    meta = cyan dim   # cyan
    frag = yellow dim  # yellow
    old = red       # red
    new = green       # green      

[color "status"]
    added = green     # green
    changed = red   # red (light)
    untracked = red dim # red
    branch = magenta bold # blue

[color "branch"]
    current = magenta blink # blue
    local = yellow         # yellow
    remote = yellow dim        # light yellow
[merge]
    tool = vimdiff
[diff]
	tool = vimdiff
[core]
    editor = vim
    excludesfile = ~/.gitignore_global
[difftool]
	prompt = false
[alias]
	d = difftool
