[user]
    name = Venkata Rakesh Anne
    email = AVRC26@gmail.com

[color]
    ui = true
    diff = auto
    status = auto
    branch = auto

[color "diff"]
    meta = cyan dim
    frag = yellow dim
    old = magenta
    new = green

[color "status"]
    added = green
    changed = magenta
    branch = cyan bold blink

[color "branch"]
    current = blue blink
    local = yellow
    remote = magenta
[merge]
    tool = vimdiff
[diff]
	tool = vimdiff
[core]
    editor = vim
    excludesfile = ~/.gitignore_global
[difftool]
	prompt = false
[alias]
	d = difftool
